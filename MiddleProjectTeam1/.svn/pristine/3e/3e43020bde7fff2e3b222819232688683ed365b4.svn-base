package kr.or.ddit.vo;

public class MaterialVO {
	private int material_no;
	private String material_title;
	private String material_des;
	private String material_grade;
	private String material_level;
	private int material_file;
	// 학습지 문제, 파일 불러오는 맴버변수
	private int question_no;
	private String question_content;
	private String question_answer;
	private String question_solve;
	
	private int files_no;
	private String files_name;
	
	
	public int getMaterial_no() {
		return material_no;
	}
	public void setMaterial_no(int material_no) {
		this.material_no = material_no;
	}
	public String getMaterial_title() {
		return material_title;
	}
	public void setMaterial_title(String material_title) {
		this.material_title = material_title;
	}
	public String getMaterial_des() {
		return material_des;
	}
	public void setMaterial_des(String material_des) {
		this.material_des = material_des;
	}
	public String getMaterial_grade() {
		return material_grade;
	}
	public void setMaterial_grade(String material_grade) {
		this.material_grade = material_grade;
	}
	public String getMaterial_level() {
		return material_level;
	}
	public void setMaterial_level(String material_level) {
		this.material_level = material_level;
	}
	public int getQuestion_no() {
		return question_no;
	}
	public void setQuestion_no(int question_no) {
		this.question_no = question_no;
	}
	public String getQuestion_content() {
		return question_content;
	}
	public void setQuestion_content(String question_content) {
		this.question_content = question_content;
	}
	public String getQuestion_answer() {
		return question_answer;
	}
	public void setQuestion_answer(String question_answer) {
		this.question_answer = question_answer;
	}
	public String getQuestion_solve() {
		return question_solve;
	}
	public void setQuestion_solve(String question_solve) {
		this.question_solve = question_solve;
	}
	public int getFiles_no() {
		return files_no;
	}
	public void setFiles_no(int files_no) {
		this.files_no = files_no;
	}
	public String getFiles_name() {
		return files_name;
	}
	public void setFiles_name(String files_name) {
		this.files_name = files_name;
	}
	public int getMaterial_file() {
		return material_file;
	}
	public void setMaterial_file(int material_file) {
		this.material_file = material_file;
	}
	
	
	
	
	
	
}
